

<section id="form" class="container text-center">
    <div class="row">
        <div class="col-lg-8 col-lg-offset-2">
          <h2>次は</h2>
            <p>自分達も面白いこと、価値のあることのために<br>誰かにお金を送ってもらおう</p>
        </div>
    </div>
</section>
<div class="col-md-8 col-md-offset-2">
<%= form_with model: grayscale, local: true do |form| %>
  <div class="form-group">
    <%= form.label :タイトル %><br>
    <%= form.text_field :title, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :理由 %><br>
    <%= form.text_field :reason, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :公開鍵 %><br>
    <%= form.text_field :p_key, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :SNS %><br>
    <%= form.text_field :sns, class: "form-control" %>
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
<% end %>
</div>
